pipeline {
    agent any

    stages {
        stage('Print Hello World - edited') {
            steps {
                sleep 11
                echo 'Hello World'
            }
        }

        stage('Edited stage - 2') {
                    steps {
                        sleep 7
                        echo 'Hello World 2'
                    }
                }

         stage('New stage - 3') {
                            steps {
                                sleep 3
                                echo 'Hello World 3'
                            }
                        }
    }
}
