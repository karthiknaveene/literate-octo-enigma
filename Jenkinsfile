pipeline {
    agent any

    stages {
        stage('Print Hello World - edited') {
            steps {
                sleep 1
                echo 'Hello World'
            }
        }

        stage('Edited stage - 2') {
                    steps {
                        sleep 1
                        echo 'Hello World 2'
                    }
                }

         stage('New stage - 3') {
                            steps {
                                sleep 2
                                echo 'Hello World 3'
                            }
                        }

        stage('New stage - 4') {
                                    steps {
                                        sleep 3
                                        echo 'Hello World 4'
                                       
                                    }
                                }
        

        stage('New stage - 5') {
                                    steps {
                                        sleep 2
                                        echo 'Hello World 5'
                                       
                                    }
                                }
    }
}
